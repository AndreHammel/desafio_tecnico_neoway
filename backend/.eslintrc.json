{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["standard"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "template-curly-spacing": "off",
    "array-bracket-spacing": "off",
    "computed-property-spacing": "off",
    "space-before-function-paren": "off",
    "no-extra-boolean-cast": "off"
  }
}
